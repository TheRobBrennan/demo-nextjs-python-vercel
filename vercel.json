{
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "functions": {
        "api/py/**/*.py": {
            "runtime": "python3.13"
        }
    },
    "rewrites": [
        {
            "source": "/api/py/(.*)",
            "destination": "/api/py/$1"
        }
    ]
}